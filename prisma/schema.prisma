generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  name      String
  age       Int
  phone     String
  region    String
  district  String
  createdAt DateTime @default(now())
}
